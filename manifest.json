{
    "manifest_version": 3,
    "name": "Resume Filler Extension",
    "version": "1.0",
    "description": "Effortlessly complete resumes with a single click using Resume Filler Extension.",
    "author": "lpbborges",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": ["scripting", "activeTab", "storage"],
    "action": {
        "default_popup": "popup/popup.html"
    }
}
